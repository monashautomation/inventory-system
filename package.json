{
    "name": "package.json",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "overrides": {
        "@trpc/server": "11.6.0"
    },
    "scripts": {
        "db": "docker-compose up db",
        "dev": "concurrently \"vite\" \"bun run dev:backend\"",
        "start": "concurrently \"vite preview --host\" \"bun run dev:backend\"",
        "build": "tsc -b && vite build",
        "generate": "bun ./server/generateData.ts",
        "seed:printers": "bun ./server/seedPrinters.ts",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "lint": "eslint src --ext .ts,.tsx --fix",
        "gitlint": "FILES=$(git diff --cached --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx)$'); if [ -n \"$FILES\" ]; then eslint $FILES --fix; else echo 'No TypeScript files to lint'; fi",
        "preview": "vite preview",
        "dev:backend": "bun --hot server/index.ts",
        "backend": "bun --hot server/index.ts",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "prepare": "husky",
        "precommit": "bun run lint && bun run format && bun run test"
    },
